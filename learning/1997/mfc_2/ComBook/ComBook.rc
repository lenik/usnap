        MENUITEM "&Del Entry\tCtrl+D",          ID_EDIT_DEL
        MENUITEM "&Modify Entry\tCtrl+M",       ID_EDIT_MODIFY
    END
    POPUP "查看(&V)"
    BEGIN
        MENUITEM "工具栏(&T)",                  ID_VIEW_TOOLBAR
        MENUITEM "状态栏(&S)",                  ID_VIEW_STATUS_BAR
